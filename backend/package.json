{
  "name": "agentic-travel-agent-backend",
  "version": "1.0.0",
  "description": "Node.js TypeScript backend API for Agentic Travel Agent",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:dev": "tsx src/server.ts",
    "lint": "eslint . --ext .ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "db:migrate": "npx tsx src/scripts/migrate.ts migrate",
    "db:status": "npx tsx src/scripts/migrate.ts status",
    "db:reset": "npx tsx src/scripts/migrate.ts reset",
    "db:migrate-data": "npx tsx src/scripts/migrateFileData.ts migrate",
    "db:preview-data": "npx tsx src/scripts/migrateFileData.ts preview",
    "db:backup-data": "npx tsx src/scripts/migrateFileData.ts backup"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@sendgrid/mail": "^8.1.5",
    "@types/cron": "^2.0.1",
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.7",
    "@types/pg": "^8.15.4",
    "@types/validator": "^13.15.2",
    "amadeus": "^8.1.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "cron": "^4.3.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "isomorphic-dompurify": "^2.25.0",
    "joi": "^17.13.3",
    "jsdom": "^25.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "node-pg-migrate": "^8.0.3",
    "pg": "^8.16.2",
    "proper-lockfile": "^4.1.2",
    "redis": "^5.5.6",
    "uuid": "^10.0.0",
    "validator": "^13.15.15",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.12.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/connect-redis": "^0.0.23",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.4",
    "@types/node-cron": "^3.0.11",
    "@types/proper-lockfile": "^4.1.4",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "colors": "^1.4.0",
    "eslint": "^9.11.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
